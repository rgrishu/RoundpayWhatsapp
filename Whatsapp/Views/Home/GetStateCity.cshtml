@model IEnumerable<Whatsapp.Models.Data.City>;
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">State</th>
            <th scope="col">City</th>

        </tr>
    </thead>
    <tbody>
        @{
            int count = 1;
            foreach (var item in Model)
            {

                <tr>
                            <th scope="row">@count</th>
                    <td>@item.StateID</td>
                    <td>@item.CityName</td>

                </tr>
                count= count+1;
            }

        }

    </tbody>
</table>


